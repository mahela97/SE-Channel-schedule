    .bg-model {
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.7);
      /* opacity: inherit; */
      position: absolute;
      top: 0px;
      left: 0px;
      display: flex;
      justify-content: center;
      align-items: center;
      display: none;
    }

    .model-content {
      width: 500px;
      height: 250px;
      background-color: white;
      border-radius: 4px;
      text-align: center;
      padding: 20px;
      position: relative;
    }

    .ProgramList {
      width: 100%;
      border-collapse: collapse;
      table-layout: fixed;
    }

    .ProgramList THEAD {
      color: #FFFFFF;
      font-size: 20px;
    }


    .ProgramList TBODY {}

    .button {
      background-color: #202020;
    }

    .button:hover {
      background-color: black;
    }

    .ProgramList TD {
      border: none;
      padding: 6px;
      text-align: center;
    }

    .ProgramList TBODY TR TD:not(:nth-child(3)) {
      background-color: #343a40;
      font-weight: bold;
    }

    .ProgramList TBODY TR TD:last-child {
      background-color: none !important;
      font-weight: bold;
    }


    .button:hover {
      background-color: black;
      color: white;
    }


    .myTimetable {
      width: 100%;
      border-collapse: collapse;
      table-layout: fixed;
    }

    .myTimetable THEAD {
      color: #FFFFFF;
    }

    .myTimetable TBODY {
      background-color: #AAAAFF;
    }

    .myTimetable TD {
      border: 1px solid black;
      padding: 6px;
      text-align: center;
    }

    .myTimetable TBODY TR TD:first-child {
      background-color: cornflowerblue;
      font-weight: bold;
    }

    .break,
    .lunch {
      background-color: #FF0066 !important;
      font-style: italic;
    }

    .subject {
      font-size: 10pt;
      margin-bottom: 4px;
    }

    .room {
      font-size: 10pt;
      font-style: italic;
    }

    .channel {
      position: absolute;
      left: 10px;
      font-weight: bold;
      font-size: 18px;
      text-shadow: 2px 2px 10px black;
    }

    .card_channel {
      height: 275px;
      width: 400px;
      box-shadow: 6px 6px 3px black;
      margin-left: auto;
      margin-right: auto;
      border-radius: 20px;
    }

    .container {
      width: 80% !important;

    }

    td {
      width: 100%;
    }

    tbody td {
      vertical-align: middle;
    }

    .wrap {
      width: 100% !important;
    }

    thead tr th {
      text-align: center;
      font-size: 20px;
      text-shadow: 1px 1px 1px black;
    }

    .heading {
      text-shadow: 2px 2px 5px white;
      text-decoration: underline;
    }

    .channel_col {
      padding-bottom: 75px;
      margin-bottom: 5opx;

    }

    .card_channel {
      filter: grayscale(50%);
      -webkit-filter: grayscale(50%);
      -o-filter: grayscale(50%);
      -moz-filter: grayscale(50%);

    }

    .card_channel:hover {
      filter: grayscale(0%);
      -webkit-filter: grayscale(0%);
      -o-filter: grayscale(0%);
      -moz-filter: grayscale(0%);

    }

    .c_body:hover::after {
      content: "Click the button";
      margin: auto;

    }

    td {
      word-wrap: break-word !important;
    }



    .feed {
      width: 100%;
      padding: 15px;
      margin: 5px 0 22px 0;
      display: inline-block;
      border: none;
      background: #f1f1f1;
    }



    /* Small devices (landscape phones, 576px and up)*/
    @media (max-width: 576px) {

      .card_channel {
        width: 250px;
        height: 200px;
      }

      table {
        width: 100%;
      }

      .c_body.c_body:hover::after {
        padding: 0px;
        content: " click";
      }

      .heading {
        font-size: 1.5em;
        font-weight: bolder;
      }

      td button {
        padding: 0px 5px 0px 5px !important;
        height: 50px;
        font-size: 12px !important;

      }

      td {
        font-size: 15px;
      }


    }

    /* Medium devices (tablets, 768px and up)*/
    @media (max-width: 768px) {

      thead tr th {

        font-size: 10px;
      }

    }

    /* Large devices (desktops, 992px and up)*/
