      color: #FFFFFF;
    }

    .break,
    .lunch {
      background-color: #FF0066 !important;
      font-style: italic;
    }

    .subject {
      font-size: 10pt;
      margin-bottom: 4px;
    }

    .room {
      font-size: 10pt;
      font-style: italic;
    }

    .channel {
      position: absolute;
      left: 10px;
      font-weight: bold;
      font-size: 18px;
      text-shadow: 2px 2px 10px black;
    }

    .card_channel {
      height: 275px;
      width: 400px;
      box-shadow: 6px 6px 3px black;
      margin-left: auto;
      margin-right: auto;
      border-radius: 20px;
    }

    .container {
      width: 80% !important;
    }



    tbody td {
      vertical-align: middle;
    }

    .wrap {
      width: 100% !important;
    }

    thead tr th {
      text-align: center;
      font-size: 15px;
      text-shadow: 1px 1px 1px black;
    }

    .heading {
      text-shadow: 2px 2px 5px white;
      text-decoration: underline;
      margin-bottom: 25px;
      font-size: 2em;
      font-weight: bolder;
    }

    .channel_col {
      padding-bottom: 75px;
      margin-bottom: 5opx;

    }

    .card_channel {
      filter: grayscale(50%);
      -webkit-filter: grayscale(50%);
      -o-filter: grayscale(50%);
      -moz-filter: grayscale(50%);
      background: url(../assets/img/cards/Channel_1.jpg);
      background-size: cover;

    }

    .card_channel:nth-child(even) {
      background-image: url(../assets/img/cards/radio2.jpg) !important;
    }

    .card_channel:hover {
      filter: grayscale(0%);
      -webkit-filter: grayscale(0%);
      -o-filter: grayscale(0%);
      -moz-filter: grayscale(0%);

    }

    .c_body:hover::after {
      content: "Click the button";
      margin: auto;

    }

    .button:hover {
      background-color: black;
      color: white;
    }

    table {
      border-collapse: collapse;

    }


    td {
      border: none;
      height: 30px;
    }

    th {
      background-color: #202020;
      color: white;
      text-decoration: none;
      border-color: white;
      height: 30px;
      vertical-align: middle;
    }

    tbody tr td {
      padding-left: 10px;
    }

    tr td:first-child {
      background-color: #efefef !important;
      color: black;
      text-align: center;
    }

    tr:nth-child(even) {
      color: black;
      background-color: #eee;
    }

    tbody tr:nth-child(odd) {
      color: black;
      background-color: white;
    }

    tfoot tr td {
      background-color: black !important;
      color: white;
      font-weight: bolder;
      text-align: center;
      vertical-align: middle;
