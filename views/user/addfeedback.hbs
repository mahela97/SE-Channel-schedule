  <section class="slide fade-6 kenBurns" data-name="case">
    <div class="content">
      <div class="container">
        <div class="wrap">


          <!-- Channel Cards start-->

          <!-- Channel Cards start end-->


          <!-- feedback table start-->

          {{#if ch}}
          <H1 class="heading">{{ch}} TV Program List</H1>


          <TABLE class="ProgramList">

            <colgroup>
              <col span="1" style="width:23%">
              </col>
              <col span="1" style="width:50%">
              </col>
              <col span="1" style="width:12%">
              </col>
              <col span="1" style="width:15%">
              </col>
            </colgroup>

            <THEAD>
              <TR>
                <TH>Program</TH>
                <TH>FeedBack</TH>
                <TH>Add to Favourite</TH>
                <TH></TH>
              </TR>
            </THEAD>

            <TBODY>

              {{#each channel.programTable}}

              <form action="" method="POST">
                <TR>
                  <TD>{{this.name}}
                    <input type="hidden" value="{{this.id}}" id="proid" name="proid">
                  </TD>
                  <TD>

                    <input type="text" class="feed" style="" name="addfeedback" id="addfeedback"
                      placeholder="add feedback">
                  </TD>
                  <td>
                    {{#if this.fav}}
                    <input type="checkbox" style="height: 20px;width: 20px;" id="fav{{this.id}}" name="fav" value="fav"
                      checked>
                    {{else}}
                    <input type="checkbox" style="height: 20px;width: 20px;" id="fav{{this.id}}" name="fav" value="fav">
                    {{/if}}
                    <label for="fav{{this.id}}">&#10038;</label>
                  </td>
                  <td style="background:none">
                    <button class="button" type="submit">submit</button>
                  </td>
                </TR>

                {{#if error}}
                     <div style="font-size:20px; color:red;"><b>{{error}} </b></div>
                    {{/if}}
              </form>

              {{/each}}

            </TBODY>
          </TABLE>

          {{/if}}

          <!-- feedback table start-->
          <div class="cards">
            <div class="row ">
              {{#each channel.channel}}

              <div class="col channel_col">
                <div class="card card_channel"
                  style="background:url(/assets/img/cards/Channel_1.jpg); background-size :cover;  ">
                  <div class="card-body">
                    <span class="channel">
                      {{this}}
                    </span>
                  </div>
                  <a class="button c_body" href="/user/addfeedback/{{this}}">
                    Goto>>
                  </a>
                </div>
              </div>

              {{/each}}

            </div>
          </div>
        </div>
